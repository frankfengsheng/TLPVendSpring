<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:toDegrees="20"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="30"
        />
    <rotate
        android:startOffset="30"
        android:toDegrees="-40"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="60"
        android:fillAfter="true"
        />
    <rotate
        android:startOffset="90"
        android:toDegrees="40"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="60"
        />
    <rotate
        android:startOffset="150"
        android:toDegrees="-40"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="60"
        />
    />
    <rotate
        android:startOffset="210"
        android:toDegrees="40"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="60"
        />
    <rotate
        android:startOffset="270"
        android:toDegrees="-40"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="60"
        />
    <rotate
        android:startOffset="330"
        android:toDegrees="20"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="30"
        />

    <scale
        android:duration="330"
        android:fillAfter="false"
        android:fromXScale="0.8"
        android:fromYScale="0.8"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXScale="1.1"
        android:toYScale="1.1" />
    <alpha
        android:startOffset="420"
        android:duration="450"
        android:fromAlpha="1.0"
        android:toAlpha="0.1" />
    <translate
        android:duration="420"
        android:fromYDelta="0%p"
        android:interpolator="@android:anim/accelerate_interpolator"
        android:startOffset="550"
        android:toYDelta="108%p" />

</set>