        <item name="android:paddingBottom">5px</item>
        <item name="android:paddingTop">5px</item>
        <item name="android:paddingLeft">10px</item>
        <item name="android:paddingRight">10px</item>
        <item name="android:textSize">@dimen/btn_bkg_menu_tz</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="menu_page_button" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
