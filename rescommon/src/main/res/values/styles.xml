        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@drawable/circlebuttonstyle</item>
        <item name="android:padding">5px</item>
        <item name="android:textColorHint">@color/text_background</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/aisle_edit_text_tz</item>
        <item name="android:layout_marginTop">10px</item>
    </style>

