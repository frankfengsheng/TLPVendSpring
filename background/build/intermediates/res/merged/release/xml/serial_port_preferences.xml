<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android" android:title="Serial port setup">
<ListPreference android:key="MAINDEVICE" android:title="@string/serial_port_motherboard_serial" android:persistent="true"></ListPreference>
<ListPreference android:entries="@array/baudrates_name" android:entryValues="@array/baudrates_value" 
                android:key="MAINBAUDRATE" android:persistent="true" android:title="@string/serial_port_motherboard_baud_rate"></ListPreference>
<ListPreference android:key="SERVERDEVICE" android:title="@string/serial_port_server_serial" android:persistent="true"></ListPreference>
<ListPreference android:key="DEVICETHIRD" android:title="@string/serial_port_board_serial_3" android:persistent="true"></ListPreference>


<ListPreference android:key="PasivScanPayComPath" android:title="@string/serial_passive_pay_serial" android:persistent="true"></ListPreference>
<ListPreference android:entries="@array/baudrates_name" android:entryValues="@array/baudrates_value"
    android:key="PasivScanPayBaudrate" android:persistent="true" android:title="@string/serial_passive_pay_baud_rate"></ListPreference>


<ListPreference android:key="DEVICETEMP" android:title="@string/serial_temp_serial" android:persistent="true"></ListPreference>
<ListPreference android:entries="@array/baudrates_name" android:entryValues="@array/baudrates_value"
    android:key="TEMPBAUDRATE" android:persistent="true" android:title="@string/serial_temp_baud_rate"></ListPreference>


<ListPreference android:key="OTHER" android:title="@string/serial_port_other" android:persistent="true"></ListPreference>
<ListPreference android:entries="@array/baudrates_name" android:entryValues="@array/baudrates_value"
        android:key="OTHERBAUDRATE" android:persistent="true" android:title="@string/serial_other_baud_rate"></ListPreference>
